<h1>I LOVE BEER</h1>

<div> 
<h1>Users</h1>
<ul>
  <% @users.each do |user| %>
    <li><%= user.first %> <%= user.last %> <%= link_to "View", user_path(user)  %> <%= link_to "Edit", edit_user_path(user)  %> <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this user?"} %> </li>
  <% end %>
</ul>
</div>

